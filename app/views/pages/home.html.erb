<h1>Homepage</h1>
<p><%= @tenant.subdomain %></p>
<% @users.each do |user| %>
  <p>
    <%= user.email %>
  </p>
<% end %>
<% if user_signed_in? %>
  <li>
  <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>        
  </li>
<% else %>
  <li>
  <%= link_to('Login', new_user_session_path)  %>  
  </li>
<% end %>